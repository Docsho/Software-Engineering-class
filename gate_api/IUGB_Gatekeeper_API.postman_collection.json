{  "info": {    "name": "IUGB Gatekeeper API",    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"  },  "item": [    {      "name": "Login",      "request": {        "method": "POST",        "header": [          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"username\": \"root\",\n  \"password\": \"yourpassword\"\n}"        },        "url": "http://localhost/gate_api/public/login.php"      }    },    {      "name": "Logout",      "request": {        "method": "POST",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" }        ],        "url": "http://localhost/gate_api/public/logout.php"      }    },    {      "name": "Get Student",      "request": {        "method": "GET",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" }        ],        "url": {          "raw": "http://localhost/gate_api/public/student_get.php?student_id={{student_id}}",          "query": [            { "key": "student_id", "value": "{{student_id}}" }          ]        }      }    },    {      "name": "Add Student",      "request": {        "method": "POST",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"student_id\": \"S1254\",\n  \"first_name\": \"Alice\",\n  \"last_name\": \"Johnson\",\n  \"program\": \"Computer Science\",\n  \"level\": \"Junior\"\n}"        },        "url": "http://localhost/gate_api/public/student_add.php"      }    },    {      "name": "Update Student",      "request": {        "method": "PUT",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"student_id\": \"S1254\",\n  \"program\": \"Software Engineering\",\n  \"level\": \"Senior\"\n}"        },        "url": "http://localhost/gate_api/public/student_update.php"      }    },    {      "name": "Delete Student",      "request": {        "method": "DELETE",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"student_id\": \"S1254\"\n}"        },        "url": "http://localhost/gate_api/public/student_delete.php"      }    },    {      "name": "Create User",      "request": {        "method": "POST",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"username\": \"gate1\",\n  \"password\": \"123456\",\n  \"full_name\": \"Gatekeeper Diallo\",\n  \"role_id\": 3\n}"        },        "url": "http://localhost/gate_api/public/user_add.php"      }    },    {      "name": "Update User",      "request": {        "method": "PUT",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"user_id\": 3,\n  \"full_name\": \"Gatekeeper Traore\",\n  \"active\": 1\n}"        },        "url": "http://localhost/gate_api/public/user_update.php"      }    },    {      "name": "Change Password",      "request": {        "method": "POST",        "header": [          { "key": "Authorization", "value": "Bearer {{token}}" },          { "key": "Content-Type", "value": "application/json" }        ],        "body": {          "mode": "raw",          "raw": "{\n  \"current_password\": \"oldpass\",\n  \"new_password\": \"newpass123\"\n}"        },        "url": "http://localhost/gate_api/public/change_password.php"      }    }  ]}       